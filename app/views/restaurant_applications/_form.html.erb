<%= form_for @restaurant_application do |f| %>

  <div>
    <%= f.label :restaurant_name %>
    <%= f.text_field :restaurant_name, class:"form-control" %>
  </div>

  <div>
    <%= f.label :restaurant_doc %>
    <%= f.file_field :restaurant_doc, class:"form-control" %>
  </div>

  <div>
    <%= f.label :phone_number %>
    <%= f.text_field :phone_number, :placeholder => "Your phone number starts with +60", class:"form-control" %>
  </div>

  <!-- <div>
    <%= f.label :verified %>
    <%= f.check_box :verified, class:"form-control" %>
  </div> -->

<p>
  <div>
    <%= f.submit class:"btn btn-success" %>
  </div>
<% end %>
